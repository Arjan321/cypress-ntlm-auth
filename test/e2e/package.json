{
  "name": "e2e",
  "version": "1.0.0",
  "description": "e2e test for cypress-ntlm-auth",
  "main": "index.js",
  "scripts": {
    "prepare-e2e": "npm pack ../.. && npm i cypress-ntlm-auth*.tgz",
    "webserver": "node webserver/server.js",
    "webserver:stop": "node webserver/stop-server.js",
    "e2e": "npx cypress-ntlm run",
    "pretest": "npm run prepare-e2e",
    "test": "npm-run-all --parallel webserver e2e",
    "posttest": "npm run webserver:stop"
  },
  "author": "Björn Weström",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cypress": "^5.3.0",
    "cypress-ntlm-auth": "file:cypress-ntlm-auth-3.0.0.tgz",
    "debug": "^4.2.0",
    "express": "^4.17.1",
    "express-ntlm": "^2.5.2",
    "express-ws": "^4.0.0",
    "morgan": "^1.10.0",
    "morgan-debug": "^2.0.0",
    "npm-run-all": "^4.1.5"
  }
}
