{
  "name": "cypress-ntlm-auth",
  "version": "0.9.2",
  "description": "NTLM authentication plugin for Cypress",
  "main": "index.js",
  "scripts": {
    "proxy": "node src/proxy/index.js &",
    "launch": "node src/launcher/cypressNtlm.js",
    "pnl": "npm run proxy && npm run launch",
    "test": "mocha test/**/*.spec.js",
    "test-ts": "mocha --require ./test/tshooks.js --require source-map-support/register test/**/*.spec.ts",
    "tsc": "tsc",
    "coverage": "nyc --require ./test/tshooks.js --require source-map-support/register --include \"src\" --extension \".ts\" --exclude \"src/commands/index.d.ts\" --all npm run test-ts"
  },
  "engines": {
    "node": ">=8.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bjowes/cypress-ntlm-auth.git"
  },
  "keywords": [
    "cypress",
    "ntlm",
    "windows authentication"
  ],
  "bin": {
    "cypress-ntlm": "./src/launcher/cypressNtlm.js",
    "ntlm-proxy": "./src/proxy/index.js",
    "ntlm-proxy-exit": "./src/launcher/ntlmProxyExit.js"
  },
  "author": "Björn Weström",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bjowes/cypress-ntlm-auth/issues"
  },
  "homepage": "https://github.com/bjowes/cypress-ntlm-auth#readme",
  "dependencies": {
    "appdata-path": "^1.0.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "fs-extra": "^7.0.1",
    "get-port": "^4.2.0",
    "http-mitm-proxy": "^0.7.0",
    "http-proxy-agent": "^2.1.0",
    "httpntlm": "^1.7.6",
    "https-proxy-agent": "^2.2.1",
    "inversify": "^5.0.1",
    "mkdirp": "^0.5.1",
    "node-cleanup": "^2.1.2",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@fluffy-spoon/substitute": "^1.87.0",
    "@types/assert": "^1.4.2",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/debug": "^4.1.2",
    "@types/express": "^4.16.1",
    "@types/express-ntlm": "^2.3.0",
    "@types/fs-extra": "^5.0.5",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.14.1",
    "@types/node-cleanup": "^2.1.0",
    "@types/node-forge": "^0.8.1",
    "@types/sinon": "^7.0.10",
    "@types/tunnel": "0.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.15.3",
    "express-ntlm": "^2.3.0",
    "is-port-reachable": "^2.0.0",
    "mocha": "^6.0.2",
    "node-forge": "^0.8.2",
    "proxyquire": "^2.1.0",
    "sinon": "^7.3.0",
    "source-map-support": "^0.5.11",
    "ts-node": "^8.0.3",
    "tunnel": "0.0.6",
    "typescript": "^3.3.4000"
  }
}
