{
  "name": "cypress-ntlm-auth",
  "version": "0.6.0",
  "description": "NTLM authentication plugin for Cypress",
  "main": "index.js",
  "scripts": {
    "proxy": "node src/proxy/index.js &",
    "launch": "node src/launcher/cypressNtlm.js",
    "pnl": "npm run proxy && npm run launch",
    "test": "mocha --recursive"
  },
  "engines": {
    "node": ">=8.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bjowes/cypress-ntlm-auth.git"
  },
  "keywords": [
    "cypress",
    "ntlm",
    "windows authentication"
  ],
  "bin": {
    "cypress-ntlm": "./src/launcher/cypressNtlm.js",
    "ntlm-proxy": "./src/proxy/index.js",
    "ntlm-proxy-exit": "./src/launcher/ntlmProxyExit.js"
  },
  "author": "Björn Weström",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bjowes/cypress-ntlm-auth/issues"
  },
  "homepage": "https://github.com/bjowes/cypress-ntlm-auth#readme",
  "dependencies": {
    "appdata-path": "^1.0.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "get-port": "^4.1.0",
    "http-mitm-proxy": "^0.6.1",
    "httpntlm": "^1.7.6",
    "mkdirp": "^0.5.1",
    "node-cleanup": "^2.1.2"
  },
  "devDependencies": {
    "eslint": "^5.12.1",
    "express-ntlm": "^2.3.0",
    "is-port-reachable": "^2.0.0",
    "mocha": "^5.2.0",
    "proxyquire": "^2.1.0",
    "sinon": "^7.2.3"
  }
}
